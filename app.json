{
  "expo": {
    "name": "ST Ford Alternator",
    "slug": "stford-alternator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "stford",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C70336"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kashan902.stfordalternator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#C70336"
      },
      "package": "com.kashan902.stfordalternator"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "95c9aba1-e539-4764-9935-04e92a00f5fe"
      }
    },
    "owner": "kashan902"
  }
}
